{
  "expo": {
    "name": "TOX",
    "slug": "TOX",
    "version": "1.2.0",
    "icon": "./assets/mainLogo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/mainLogo.png",
      "resizeMode": "contain",
      "backgroundColor": "#EFD6BE"
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/notificationLogo.png",
          "color": "#EFD6BE",
          "sounds": [
            "./assets/sharp.wav"
          ]
        }
      ]
    ],
    "updates": {
      "fallbackToCacheTimeout": 0,
      "url": "https://u.expo.dev/467764a2-4d68-4120-92a1-e926f852a1dc"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "infoPlist": {
        "UIBackgroundModes": [
          "audio"
        ]
      },
      "supportsTablet": true,
      "bundleIdentifier": "com.aryanb.TOX"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/mainLogo.png",
        "backgroundColor": "#EFD6BE"
      },
      "googleServicesFile": "./google-services.json",
      "package": "com.aryanb.TOX"
    },
    "extra": {
      "eas": {
        "projectId": "467764a2-4d68-4120-92a1-e926f852a1dc"
      }
    },
    "runtimeVersion": {
      "policy": "sdkVersion"
    }
  }
}
